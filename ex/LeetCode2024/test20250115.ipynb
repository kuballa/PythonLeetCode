    "\n",
    "    while l < len(left) and r < len(right):\n",
    "      if left[l] < right[r]:\n",
    "        res.append(left[l])\n",
    "        l += 1\n",
    "      else:\n",
    "        res.append(right[r])\n",
    "        r += 1\n",
    "    \n",
    "    res.extend(left[l:])\n",
    "    res.extend(right[r:])\n",
    "    return res\n",
